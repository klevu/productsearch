<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Module/etc/module.xsd">
    <module name="Klevu_Search" setup_version="2.10.3">
        <sequence>
            <module name="Magento_Indexer"/>
            <module name="Magento_Search"/>
            <module name="Magento_Catalog"/>
            <module name="Magento_CatalogSearch"/>
            <!-- FrontendJs intentionally omitted to prevent circular reference -->
            <module name="Klevu_Logger"/>
            <module name="Klevu_Metadata"/>
        </sequence>
    </module>
</config>
